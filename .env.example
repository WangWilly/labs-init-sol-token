# Environment Configuration

# Solana Configuration
# Option 1: Base58 private key (recommended for production)
SOLANA_PRIVATE_KEY_BASE58=your_base58_private_key_here

# Option 2: JSON array of private key bytes (for development)
SOLANA_PRIVATE_KEY_JSON=[123,45,67,89,...]

# Recipient Account Configuration
# Option 1: Base58 private key for recipient (recommended)
RECIPIENT_PRIVATE_KEY_BASE58=recipient_base58_private_key_here

# Option 2: JSON array of private key bytes for recipient
RECIPIENT_PRIVATE_KEY_JSON=[123,45,67,89,...]

# RPC Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com

# Token Configuration
TOKEN_DECIMALS=9
INITIAL_SUPPLY=1000000
DISTRIBUTION_BPS=2000  # 20% in basis points
SOL_AMOUNT=2

# AMM Configuration
SLIPPAGE_TOLERANCE=1  # 1% slippage tolerance

# Token Metadata Configuration (Metaplex)
TOKEN_NAME=My Demo Token
TOKEN_SYMBOL=MDT
TOKEN_IMAGE_URI=https://arweave.net/your-image-uri
TOKEN_EXTERNAL_URL=https://your-website.com

# Program Configuration (Token Launcher)
PROGRAM_ID=11111111111111111111111111111111
INITIAL_TOKEN_PRICE=0.001
MAX_TOKEN_SUPPLY=1000000

# Logging
LOG_LEVEL=info
